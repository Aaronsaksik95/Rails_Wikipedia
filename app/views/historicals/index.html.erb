<p id="notice"><%= notice %></p>

<h1>Historicals</h1>

<table>
  <thead>
    <tr> 
      <th>Article_id</th>
      <th>Title</th>
      <th>Content</th>
      <th>User_id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @historicals.each do |historical| %>
      <tr>
        <td><%= historical.article.id %></td>
        <td><%= historical.title %></td>
        <td><%= historical.content %></td>
        <td><%= historical.user.id %></td>
        <td><%= link_to 'Show', historical %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Historical', new_historical_path %>
